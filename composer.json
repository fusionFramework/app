				"version": "3.3.0",
				"source": {
					"url": "https://github.com/kohana-minion/tasks-migrations.git",
					"type": "git",
					"reference": "3.3/develop"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "zombor/kostache",
				"type": "kohana-module",
				"version": "4.0.2",
				"source": {
					"url": "https://github.com/zombor/KOstache.git",
					"type": "git",
					"reference": "master"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "morgan/kohana-paginate",
				"version": "0.3.0",
